NAME=ctable
LOCAL=`kpsewhich --expand-var '$TEXMFLOCAL'`/tex/latex

echo y |tex $NAME.ins >/dev/null
pdflatex --interaction=batchmode $NAME.dtx > /dev/null
makeindex -q -s gind.ist -o ctable.ind ctable.idx
pdflatex --interaction=batchmode $NAME.dtx > /dev/null
sudo cp $NAME.{dtx,sty,pdf} $LOCAL
mk -c $NAME
sudo mktexlsr

# $Id$

